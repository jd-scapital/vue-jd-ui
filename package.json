{
  "name": "vue-jd-ui",
  "version": "1.0.0-beta.17",
  "description": "invest/cas/ai-bank's common component",
  "main": "lib/vue-jd-ui.common.js",
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "scripts": {
    "bootstrap": "yarn || npm i",
    "build:file": "node build/bin/build-entry.js",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "build:base": "node build/bin/gen-cssfile && gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib/base.css lib/theme-chalk/base.css",
    "build:theme-webpack": "cp-cli lib/theme-chalk packages/theme-chalk/lib && cp-cli packages/theme-chalk/src/fonts packages/theme-chalk/lib/fonts && cp-cli packages/theme-chalk/src/fonts lib/theme-chalk/fonts",
    "clean": "rimraf lib && rimraf packages/*/lib && rimraf test/**/coverage",
    "dist:webpack": "npm run clean && npm run build:file && npm run lint && webpack --config build/webpack.conf.js && webpack --config build/webpack.common.js && webpack --config build/webpack.component.js && npm run build:utils && npm run build:base && npm run build:theme-webpack",
    "lint": "eslint src/**/*.js test/**/*.js packages/**/*.js packages/**/*.vue build/**/*.js --quiet",
    "pub": "npm run bootstrap && sh build/git-release.sh && sh build/release.sh",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "http://git.office.scapital.com/front/vue-jd-ui"
  },
  "keywords": [
    "jd-ui",
    "vue-jd-ui",
    "invest",
    "cas",
    "ai-bank"
  ],
  "author": "yanfeng@scapital.com, siqing@scapital.com",
  "license": "ISC",
  "unpkg": "lib/index.js",
  "style": "lib/theme-chalk/index.css",
  "dependencies": {
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "clipboard": "^2.0.4",
    "dayjs": "^1.8.14",
    "deepmerge": "^3.2.0",
    "echarts": "^4.2.1",
    "normalize-wheel": "^1.0.1",
    "numeral": "^2.0.6",
    "resize-observer-polyfill": "^1.5.1",
    "swiper": "^4.5.0",
    "throttle-debounce": "^2.1.0",
    "v-click-outside": "^2.1.3",
    "vue-swiper-2.x": "^1.0.3"
  },
  "peerDependencies": {
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/eslint-config-standard": "^4.0.0",
    "autoprefixer": "^9.6.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "cp-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^5.0.4",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-vue": "^5.2.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.0.0",
    "fs": "0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "html-loader": "^0.5.5",
    "json-loader": "^0.5.7",
    "json-templater": "^1.2.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-salad": "^2.0.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.1.2",
    "vue": "2.5.21",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "2.5.21",
    "vue-template-es2015-compiler": "^1.6.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.1.11",
    "webpack-node-externals": "^1.7.2"
  }
}
